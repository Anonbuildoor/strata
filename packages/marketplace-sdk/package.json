{
  "name": "@strata-foundation/marketplace-sdk",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "3.10.1",
  "description": "Interface to the strata marketplace",
  "repository": {
    "type": "git",
    "url": "https://github.com/StrataFoundation/strata"
  },
  "main": "./lib/esm/src/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/esm/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "import": "./lib/esm/src/index.js",
    "require": "./lib/cjs/src/index.js"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "clean": "npx shx mkdir -p lib && npx shx rm -rf lib",
    "package": "npx shx mkdir -p lib/cjs lib/esm && echo '{\"type\":\"commonjs\"}' | npx json > ./lib/cjs/package.json && echo '{\"type\":\"module\"} ' | npx json > ./lib/esm/package.json",
    "prebuild": "pnpm run clean && pnpm run package"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@project-serum/anchor": "^0.24.2",
    "@solana/spl-token": "0.1.8",
    "@solana/web3.js": "^1.43.4",
    "@strata-foundation/fungible-entangler": "workspace:^3.10.1",
    "@strata-foundation/spl-token-bonding": "workspace:^3.10.1",
    "@strata-foundation/spl-token-collective": "workspace:^3.10.1",
    "@strata-foundation/spl-utils": "workspace:^3.10.1",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "git-format-staged": "^2.1.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4"
  },
  "gitHead": "ff670e6a6211209648c56dd69e5f5dfbda631b42"
}
